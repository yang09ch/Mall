<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kgc.mapper.UserMapper">
<insert id="addUser" parameterType="com.kgc.pojo.User">
INSERT INTO `user`(userName,userPassword,userNickName,userGender,userBirthday,userHomePlace)
 VALUES(#{userName},#{userPassword},#{userNickName},#{userGender},#{userBirthday},#{userHomePlace})
</insert>
    <select id="getUserLogin" resultType="com.kgc.pojo.User">
        select * from `user` where userName=#{userName} and userPassword=#{userPassword}
    </select>
</mapper>
